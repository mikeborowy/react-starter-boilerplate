{
  "name": "react-starter-boilerplate",
  "version": "1.0.0",
  "description": "Simple small weather app written in react",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel lint-watch serv-src",
    "serv-src": "babel-node ./server/srcServer.js",
    "serv-public": "babel-node ./server/publicServer.js",
    "lint": "esw ./webpack/webpack.config.* src server",
    "lint-watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec src/tests/testSetup.js src/**/*.test.js src/tests/**/*.test.js",
    "test-watch": "npm run mocha -- --watch",
    "build": "npm-run-all clean-public test build-js build-html",
    "build-js": "babel-node buildTools/build.js",
    "build-html": "babel-node buildTools/buildHtml.js",
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "rimraf ./public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "dependencies": {
